CC=/opt/ansic/bin/cc
MT_CC=/opt/ansic/bin/cc
XCFLAGS0=-ldld -lc -Wp,-H200000 -Wl,-a,archive_shared -DAUTH_DBM_LOG +z -Wl,+k -D_LARGEFILE64_SOURCE
XCFLAGS=${XCFLAGS0} 
