RANLIB=/usr/bin/ranlib
INSTALL=${TOP_SRCDIR}/pinstall/pinstall
RM=/bin/rm
CP=/bin/cp
AR=/bin/ar
AS=/usr/ccs/bin/as
MV=/bin/mv
LD=/bin/ld   
LEX=/opt/langtools/bin/lex
DBM=/usr/lib/hpux32/libndbm.so
PAM_CFLAGS=-b +z -Wl,+k
PAMLIBS=/usr/lib/hpux32/libpam.so
YACC=/opt/langtools/bin/yacc
